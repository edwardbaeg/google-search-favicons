{
  "manifest_version": 2,
  "name": "Google Search Favicons",
  "version": "0.1",
  "description": "Adds favicons next to Google search results",

  "icons": {
    "128": "icon128.png",
    "48": "icon48.png",
    "16": "icon16.png"
  },

  "content_scripts": [{
    "matches":["*://*.google.com/*", "https://developer.chrome.com/*"],
    "js":["lib/jquery-3.3.1.min.js", "main.js"],
    "css":["styles.css"]
  }]
}